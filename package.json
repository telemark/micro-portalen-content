  },
  "main": "index.js",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "micro",
    "dev": "micro-dev",
    "deploy": "npm test && now && now alias",
    "build-tags": "node utils/build-tags-mapping.js",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "microservice",
    "micro",
    "content",
    "portalen",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/micro-portalen-content.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-portalen-content/issues"
  },
  "homepage": "https://github.com/telemark/micro-portalen-content#readme",
  "now": {
    "alias": [
      "content.portalen.win"
    ],
    "type": "npm",
    "dotenv": "production.env",
    "engines": {
      "node": "8.10.0"
    }
  },
  "devDependencies": {
    "ava": "0.25.0",
    "coveralls": "3.0.0",
    "nsp": "3.2.1",
    "micro-dev": "2.2.2",
    "nyc": "11.6.0",
