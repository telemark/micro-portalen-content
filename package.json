{
  "name": "micro-portalen-content",
  "description": "Returns content for portalen",
  "version": "3.1.1",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
